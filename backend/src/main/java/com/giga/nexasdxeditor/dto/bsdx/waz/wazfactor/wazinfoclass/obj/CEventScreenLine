int __thiscall CEventScreenLine::Read(int this, int Buffer)
{
  int v2; // esi
  int result; // eax
  int i; // ebx

  v2 = Buffer;
  read_spm_frame(*(this + 4), Buffer);
  File::read(v2, (this + 8), 4);
  File::read(v2, (this + 12), 4);
  File::read(v2, (this + 16), 4);
  File::read(v2, (this + 24), 4);
  File::read(v2, (this + 28), 4);
  File::read(v2, (this + 32), 4);
  File::read(v2, (this + 36), 4);
  File::read(v2, (this + 40), 4);
  File::read(v2, (this + 44), 4);
  result = File::read(v2, (this + 48), 4);
  for ( i = 0; i < 11; ++i )
  {
    if ( i == 3 )
    {
      result = File::read(v2, &Buffer, 4);
      if ( Buffer )
        result = (*(**(this + 20) + 24))(*(this + 20), v2);
    }
  }
  return result;
}