int __thiscall CEventScreenEffect::Read(int this, int Buffer)
{
  int v2; // esi
  int result; // eax
  int i; // ebx

  v2 = Buffer;
  read_spm_frame(*(this + 4), Buffer);
  File::read(v2, (this + 8), 4);
  File::read(v2, (this + 12), 4);
  File::read(v2, (this + 16), 4);
  result = File::read(v2, (this + 20), 4);
  for ( i = 0; i < 5; ++i )
  {
    if ( i == 4 )
    {
      result = File::read(v2, &Buffer, 4);
      if ( Buffer )
        result = (*(**(this + 24) + 24))(*(this + 24), v2);
    }
  }
  return result;
}